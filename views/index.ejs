<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <table>
      <thead>
        <tr>
          <th>User</th>
          <th>Post</th>
          <th>Date</th>
        </tr>
      </thead>
      <tbody>
        <% messages.forEach(message => { %>
        <tr>
          <td><%= message.user %></td>
          <td><%= message.text %></td>
          <td><%= message.added %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
